<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="http://www.w3.org/2005/Atom">
 
 <title>Марксистская группа "Классовая политика"</title>
 <link href="https://mgkp.github.io/blog/atom.xml" rel="self"/>
 <link href="https://mgkp.github.io/blog"/>
 <updated>{{ site.time | date_to_xmlschema }}</updated>
 <id>https://mgkp.github.io/blog</id>
 <author>
   <name>МГКП</name>
   <email>periodico.trabajadores@gmail.com</email>
 </author>

 {% for post in site.posts %}
 <entry>
   <title>{{ post.title }}</title>
   <link href="https://mgkp.github.io{{ post.url }}"/>
   <updated>{{ post.date | date_to_xmlschema }}</updated>
   <id>https://mgkp.github.io/{{ post.id }}</id>
   <content type="html">{{ post.content | xml_escape }}</content>
 </entry>
 {% endfor %}
 
</feed>
